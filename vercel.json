{
  "routes": [
    {
      "src": "/static/(.*)\\.js",
      "headers": { "content-type": "application/javascript" },
      "continue": true
    },
    {
      "src": "/(.*)\\.js",
      "headers": { "content-type": "application/javascript" },
      "continue": true
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
